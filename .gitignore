// method actionPerformed()
// to get the action performed
// by the user and act accordingly
public void actionPerformed(ActionEvent e)
{
	if (e.getSource() == sub) {
		if (term.isSelected()) {
			String data1;
			String data
				="---------------BNM Institute of Technology---------------\n"+ "Name : "
				+ tname.getText() + "\n"
				+ "Mobile : "
				+ tmno.getText() + "\n";
			if (male.isSelected())
				data1 = "Gender : Male"
						+ "\n";
			else
				data1 = "Gender : Female"
						+ "\n";
			String data2
				= "DOB : "
				+ (String)date.getSelectedItem()
				+ "/" + (String)month.getSelectedItem()
				+ "/" + (String)year.getSelectedItem()
				+ "\n";
			String data3 = "Address : " + tadd.getText()+"\n";
			String data4;
			if(sel1.isSelected())
				data4="Current Year: 1"+" \n";
			else if(sel2.isSelected())
				data4="Current Year: 2"+" \n";
			else if(sel2.isSelected())
				data4="Current Year: 3 "+"\n";
			else
				data4="Current Year: 4"+" \n";
			
			String data5
			="Course : "+(String)courses.getSelectedItem()+ "\n";
			String data6
			="\n"+"-------------Fees---------------\n\n";
			String data8="Infastructure Fees = 35,000.00"
							+ "\n"
							+ "Library Fees = 6,000.00";
			int fin,dis,inf=35000,lib=6000;
			String data7;
			if(merit.isSelected())
			{
				dis=96000;
				data7
				="Course fees = "+dis+"(After deducting 20%)"
						+ "\n";	
			}
			else if(second.isSelected())
			{
				dis=108000;
			data7
				="Course fees = "+dis+"(After deducting 10%"
						+ "\n";
			}
			else
			{
				dis=120000;
			data7
				="Course fees = "+dis
						+ "\n";
			}
			
			int trans;
			String data11 = null;
			if(km1.isSelected())
			{
				trans=10000;
				data11
				="\nTransportation fees = "+trans+"(for 5-10km)"+"\n";
			}
			else if(km2.isSelected())
			{
				trans=20000;
				data11
				="\nTransportation fees = "+trans+"(for 10-20km)"+"\n";
			}
			else if(km3.isSelected())
			{
				trans=30000;
				data11
				="\n"+"Transportation fees = "+trans+"(for 20 or more km)"+"\n";
			}
			else
			{
				trans=0;
			}
			fin=dis+inf+lib+trans;
			String data9
			="\n"+"--------------------------"+"\n" ;
			String data10
			="Total = "+fin;
			tout.setText(data + data1 + data2 + data3+data4+data5+data6+data7+data8+data11+data9+data10);
			tout.setEditable(false);
			res.setText("Registration Successfully..");
		}
		else {
			tout.setText("");
			resadd.setText("");
			res.setText("Please accept the"
						+ " terms & conditions..");
		}
	}

	else if (e.getSource() == reset) {
		String def = "";
		tname.setText(def);
		tadd.setText(def);
		tmno.setText(def);
		res.setText(def);
		tout.setText(def);
		term.setSelected(false);
		date.setSelectedIndex(0);
		month.setSelectedIndex(0);
		year.setSelectedIndex(0);
		resadd.setText(def);
		courses.setSelectedIndex(0);
	}
}
